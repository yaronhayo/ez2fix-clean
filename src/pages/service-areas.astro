      // Change cursor to pointer for clickable markers
      marker.addListener('mouseover', () => {
        marker.setOptions({ 
          cursor: 'pointer'
        });
      });
    });

    // Add resize listener for responsive zoom
    window.addEventListener('resize', () => {
      const newZoom = getResponsiveZoom();
