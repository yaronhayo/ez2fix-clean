  // Secure API key retrieval function
  async function getSecureApiKey() {
    try {
      const response = await fetch('/api/maps-key');
      if (!response.ok) {
        throw new Error(`API key endpoint failed: ${response.status}`);
      }
      
      const data = await response.json();
      if (data.success && data.apiKey) {
        return data.apiKey;
      } else {
        throw new Error(data.error || 'Invalid API key response');
      }
    } catch (error) {
      console.error('Failed to fetch API key:', error);
      return null;
    }
  }

  // Primary load function with centralized Google Maps loading
  async function loadGoogleMapsAPI() {
    try {
      const apiKey = await getSecureApiKey();
      if (!apiKey) {
        throw new Error('Failed to retrieve API key');
      }

      console.log('🔄 Loading Google Maps with centralized loader...');
      
      // Use the global maps loader to prevent duplicates
      await window.EZ2FIX_MAPS.loadAPI(apiKey);
      console.log('✅ Google Maps API loaded successfully');
      initServiceAreasMap();
      
    } catch (error) {
      console.error('❌ Google Maps loading failed:', error);
      showMapError(error.message);
    }
  }


  function showMapError(errorDetails = null) {
    const loadingElement = document.getElementById('map-loading');
    if (loadingElement) {
      // Enhanced error reporting for debugging
      console.error('🗺️ Map loading failed:', errorDetails);
      
      let errorMessage = `
        <div class="text-lg font-semibold mb-2" style="color: #1D1912;">Interactive Map Temporarily Unavailable</div>
