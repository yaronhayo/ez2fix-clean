    <!-- Final CTA - Clean and Direct -->
    <section class="py-16 bg-gradient-to-r from-ez2fix-primary to-ez2fix-gold">
      <div class="container mx-auto px-4">
        <div class="max-w-4xl mx-auto text-center">
          
          <h2 class="text-3xl md:text-4xl font-bold mb-6 text-ez2fix-dark">
            Ready to Schedule Garage Door Repair?
          </h2>
          <p class="text-xl mb-8 text-ez2fix-dark max-w-2xl mx-auto">
            Don't wait - book garage door service New Jersey's reliable professionals provide. 
            Your garage door service appointment is just one click away with our quality commitment guarantee.
          </p>
          
          <!-- Primary Actions -->
          <div class="flex flex-col sm:flex-row gap-4 justify-center items-center max-w-lg mx-auto">
            <a 
              href="#main-content"
              class="w-full sm:w-auto px-8 py-4 bg-ez2fix-dark text-ez2fix-primary font-bold rounded-lg hover:bg-ez2fix-brown hover:text-ez2fix-cream transition-all duration-300 shadow-lg hover:shadow-xl text-center"
            >
              Schedule Garage Door Repair Now
            </a>
            <span class="text-ez2fix-dark font-semibold">or</span>
            <a 
              href={siteConfig.links.phone}
              class="w-full sm:w-auto px-8 py-4 border-2 border-ez2fix-dark text-ez2fix-dark font-bold rounded-lg hover:bg-ez2fix-dark hover:text-ez2fix-primary transition-all duration-300 shadow-lg hover:shadow-xl text-center"
            >
              Call {siteConfig.business.phone}
            </a>
          </div>
        </div>
