---
import BaseLayout from '@/layouts/BaseLayout.astro';
import HeaderClean from '@/components/layout/HeaderClean.astro';
import FooterClean from '@/components/layout/FooterClean.astro';
import BookingFormClean from '@/components/ui/BookingFormClean.astro';
import FAQComponentClean from '@/components/ui/FAQComponentClean.astro';
import Breadcrumbs from '@/components/ui/Breadcrumbs.astro';
import { siteConfig } from '@/config/site';

// Commercial scenarios - Problem/Solution/Result format
const commercialScenarios = [
  {
    scenario: "Loading Dock Downtime",
    problem: "Your loading dock door failed during peak shipping hours. Trucks are waiting, freight is piling up, and every minute costs money in lost productivity.",
    solution: "Emergency commercial service with industrial-grade parts, priority response, and experienced technicians who understand business operations.",
    benefit: "Dock restored within hours with minimal revenue loss - plus preventive maintenance plan to prevent future disruptions.",
    icon: "dock"
  },
  {
    scenario: "High-Cycle Door Failure",
    problem: "Your warehouse door operates 200+ cycles daily. It's failing after years of heavy use, threatening operational continuity and employee safety.",
    solution: "Complete replacement with high-cycle commercial door rated for 100,000+ cycles, industrial opener, and commercial-grade components.",
    benefit: "Reliable operation handling high traffic volume with 5-year commercial warranty and included maintenance schedule.",
    icon: "cycle"
  },
  {
    scenario: "Energy Cost Concerns",
    problem: "Your facility's old uninsulated doors leak climate control, driving up energy costs and making temperature-sensitive areas unusable.",
    solution: "Professional installation of R-16 insulated commercial doors with thermal breaks, weather sealing, and energy-efficient design.",
    benefit: "Energy costs reduced 20-30% annually while maintaining consistent temperature control for inventory and workers.",
    icon: "energy"
  }
];

// Why professional commercial service matters
const whyCommercial = [
  {
    title: "Business Continuity Focus",
    description: "We understand downtime costs money. Our commercial service prioritizes speed and efficiency to get your operations back online fast with minimal business disruption.",
    icon: "business",
    stat: "2-4 hr response"
  },
  {
    title: "Industrial-Grade Solutions",
    description: "Commercial facilities need heavy-duty components rated for high-cycle operation. We use industrial-grade parts designed for demanding commercial environments.",
    icon: "industrial",
    stat: "100K+ cycle doors"
  },
  {
    title: "Safety & Compliance",
    description: "Commercial doors must meet OSHA and local code requirements. We ensure full regulatory compliance and proper safety system integration for liability protection.",
    icon: "safety",
    stat: "Full code compliance"
  },
  {
    title: "Preventive Maintenance",
    description: "Regular maintenance prevents unexpected failures that disrupt business. Our service contracts extend equipment life and catch problems before they become emergencies.",
    icon: "maintenance",
    stat: "Scheduled service"
  }
];

// Commercial services with taglines
const commercialServices = [
  {
    title: "Industrial Door Installation & Repair",
    tagline: "Heavy-duty doors for demanding operations.",
    description: "Professional installation and repair of industrial overhead doors designed for high-cycle commercial operations. From standard sectional doors to specialized applications, we handle heavy-duty systems with ratings up to 100,000 cycles and lifting capacities for oversized openings.",
    benefits: [
      "High-cycle door systems (100K+ cycles)",
      "Heavy-duty commercial operators",
      "Insulated and non-insulated options",
      "Fire-rated door compliance",
      "5-year commercial warranty"
    ],
    image: "industrial"
  },
  {
    title: "Loading Dock Equipment Service",
    tagline: "Keep freight moving - minimize dock downtime.",
    description: "Expert service and installation of all loading dock equipment including hydraulic dock levelers, dock seals, dock shelters, vehicle restraints, and dock bumpers. We understand dock downtime means lost revenue and work quickly to restore freight operations.",
    benefits: [
      "Hydraulic dock leveler service/install",
      "Dock seal and shelter replacement",
      "Vehicle restraint systems",
      "Emergency dock repair service",
      "Preventive maintenance programs"
    ],
    image: "dock"
  },
  {
    title: "High-Speed & Specialty Door Systems",
    tagline: "Efficiency through rapid operation.",
    description: "Installation and service of high-speed roll-up doors, rapid doors, and specialty systems for facilities requiring fast operation, energy conservation, or environmental control. Ideal for high-traffic areas, temperature-controlled zones, and clean room applications.",
    benefits: [
      "Rapid roll-up door systems",
      "Energy-efficient operation",
      "Temperature zone isolation",
      "High-traffic durability",
      "Climate control optimization"
    ],
    image: "specialty"
  }
];

// Unique commercial service reviews from different NJ locations
const commercialReviews = [
  {
    text: "Our loading dock door failed Friday afternoon during peak shipping. They had a tech on-site within 2 hours and operational by evening. Professional service that understands business needs.",
    author: "Mark T.",
    location: "Clifton, NJ",
    service: "Emergency Loading Dock Repair",
    rating: 5
  },
  {
    text: "The preventive maintenance program has eliminated unexpected door failures at our warehouse. Scheduled service keeps our 12 doors running smoothly without disrupting operations.",
    author: "Jennifer R.",
    location: "Fairfield, NJ",
    service: "Commercial Maintenance Contract",
    rating: 5
  },
  {
    text: "Replaced 4 high-cycle doors in our distribution center. Professional installation completed over a weekend with zero impact on operations. The high-speed doors improved efficiency significantly.",
    author: "David M.",
    location: "Wayne, NJ",
    service: "Industrial Door Installation",
    rating: 5
  },
  {
    text: "Dock leveler service restored our freight operations fast. They had the parts on the truck and understood exactly what needed fixing. No-nonsense commercial service at its best.",
    author: "Robert K.",
    location: "Elmwood Park, NJ",
    service: "Hydraulic Dock Leveler Repair",
    rating: 5
  },
  {
    text: "The new insulated doors reduced our heating costs noticeably. Professional commercial installation with minimal disruption to our manufacturing schedule. Great team to work with.",
    author: "Lisa P.",
    location: "Fair Lawn, NJ",
    service: "Commercial Door Installation",
    rating: 5
  },
  {
    text: "Annual maintenance contract gives us peace of mind. They inspect all our doors quarterly and catch small issues before they become expensive problems. Worth every dollar.",
    author: "Michael S.",
    location: "Montclair, NJ",
    service: "Preventive Maintenance Program",
    rating: 5
  }
];

// Commercial-specific FAQs
const commercialFAQs = [
  {
    question: "What's your emergency response time for commercial service?",
    answer: "We prioritize commercial emergencies understanding downtime costs money. Our goal is 2-4 hour response time for emergency commercial calls. We carry industrial-grade parts on our trucks to complete most repairs on the first visit."
  },
  {
    question: "Do you offer preventive maintenance contracts?",
    answer: "Yes! Our commercial maintenance programs include scheduled quarterly inspections, priority emergency service, equipment lifecycle tracking, and parts discounts. Regular maintenance prevents expensive emergency repairs and extends equipment life significantly."
  },
  {
    question: "Can you handle high-cycle industrial doors?",
    answer: "Absolutely. We specialize in high-cycle commercial doors rated for 50,000-100,000+ cycles. This includes proper spring systems, heavy-duty operators, and commercial-grade tracks designed for demanding industrial environments."
  },
  {
    question: "What types of loading dock equipment do you service?",
    answer: "We service all loading dock equipment including hydraulic dock levelers, mechanical levelers, edge-of-dock levelers, dock seals, dock shelters, vehicle restraints, dock bumpers, and door seals. Our technicians understand freight operations and work to minimize dock downtime."
  },
  {
    question: "Do commercial doors have different safety requirements?",
    answer: "Yes. Commercial facilities must meet OSHA requirements, local building codes, and industry-specific regulations. We ensure proper photo-eye sensors, emergency stops, adequate lighting, proper signage, and all safety features meet current commercial standards for liability protection."
  }
];
---

<BaseLayout
  title="Commercial Garage Door Service NJ | Industrial & Loading Dock Equipment"
  description="Professional commercial garage door service in NJ. Industrial installation, loading dock equipment, high-cycle doors. Licensed #13VH13553300. Business continuity focus."
  keywords="commercial garage door service, industrial door installation, loading dock equipment, high cycle doors, commercial door repair, warehouse doors, loading dock repair, commercial overhead doors, industrial door service NJ, commercial garage door maintenance"
>
  <Fragment slot="head">
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "Service",
      "@id": "https://ez2fixllc.com/services/commercial-garage-door-service#service",
      "name": "Professional Commercial Garage Door Service & Industrial Solutions in New Jersey",
      "description": "Expert commercial garage door service for industrial facilities, loading dock equipment, and high-cycle door systems with business continuity focus.",
      "provider": {
        "@type": "LocalBusiness",
        "@id": "https://ez2fixllc.com#business",
        "name": "Ez2Fix Garage Door Services",
        "telephone": siteConfig.business.phone,
        "priceRange": "$$"
      },
      "areaServed": {
        "@type": "State",
        "name": "New Jersey"
      },
      "aggregateRating": {
        "@type": "AggregateRating",
        "ratingValue": "4.9",
        "reviewCount": "100",
        "bestRating": "5",
        "worstRating": "1"
      },
      "review": commercialReviews.map(review => ({
        "@type": "Review",
        "author": {
          "@type": "Person",
          "name": review.author
        },
        "reviewRating": {
          "@type": "Rating",
          "ratingValue": review.rating,
          "bestRating": 5
        },
        "reviewBody": review.text,
        "itemReviewed": {
          "@type": "Service",
          "name": review.service
        }
      }))
    })} />
  </Fragment>

  <HeaderClean />

  <main id="main-content">
    <Breadcrumbs
      items={[
        { label: 'Home', href: '/' },
        { label: 'Services', href: '/services' },
        { label: 'Commercial Service', href: '/services/commercial-garage-door-service' }
      ]}
    />

    <!-- Hero Section with Booking Form -->
    <section class="relative py-16 md:py-24 overflow-hidden bg-gradient-to-br from-ez2fix-dark via-ez2fix-brown to-ez2fix-dark">
      <!-- Radial gradient overlays for depth -->
      <div class="absolute inset-0 opacity-10">
        <div class="absolute inset-0 bg-[radial-gradient(circle_at_30%_50%,_var(--ez2fix-primary)_0%,_transparent_50%)]"></div>
        <div class="absolute inset-0 bg-[radial-gradient(circle_at_70%_50%,_var(--ez2fix-gold)_0%,_transparent_50%)]"></div>
      </div>

      <div class="container mx-auto px-4 relative z-10">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 items-center max-w-7xl mx-auto">

          <!-- Hero Content -->
          <div>
            <div class="inline-flex items-center px-4 py-2 bg-ez2fix-primary text-ez2fix-dark font-bold rounded-full text-sm mb-6">
              <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/>
              </svg>
              COMMERCIAL BUSINESS SOLUTIONS
            </div>

            <h1 class="text-4xl md:text-5xl lg:text-6xl font-bold mb-6">
              <span class="text-white">Commercial Door</span><br />
              <span class="text-ez2fix-primary">Service</span><br />
              <span class="text-ez2fix-gold">That Keeps You Running.</span>
            </h1>

            <p class="text-xl text-ez2fix-cream mb-8 leading-relaxed">
              Professional commercial garage door service for industrial facilities, loading docks, and high-cycle systems. Fast response that minimizes downtime and keeps operations moving.
            </p>

            <div class="flex flex-col sm:flex-row gap-4 mb-8">
              <a
                href={siteConfig.links.phone}
                class="inline-flex items-center justify-center px-8 py-4 bg-ez2fix-primary text-ez2fix-dark hover:bg-ez2fix-gold font-bold text-lg rounded-xl shadow-xl hover:shadow-2xl transition-all duration-300 hover:scale-105"
              >
                <svg class="w-5 h-5 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                </svg>
                Call {siteConfig.business.phone}
              </a>
            </div>

            <div class="grid grid-cols-3 gap-4 text-center">
              <div class="bg-white/10 backdrop-blur-sm rounded-lg p-4">
                <div class="text-2xl font-bold text-ez2fix-primary mb-1">2-4 hrs</div>
                <div class="text-sm text-ez2fix-cream">Emergency Response</div>
              </div>
              <div class="bg-white/10 backdrop-blur-sm rounded-lg p-4">
                <div class="text-2xl font-bold text-ez2fix-primary mb-1">100K+</div>
                <div class="text-sm text-ez2fix-cream">Cycle Doors</div>
              </div>
              <div class="bg-white/10 backdrop-blur-sm rounded-lg p-4">
                <div class="text-2xl font-bold text-ez2fix-primary mb-1">5 Years</div>
                <div class="text-sm text-ez2fix-cream">Commercial Warranty</div>
              </div>
            </div>
          </div>

          <!-- Booking Form with 4px border -->
          <div class="relative">
            <div class="absolute inset-0 bg-gradient-to-br from-ez2fix-primary to-ez2fix-gold rounded-2xl"></div>
            <div class="relative bg-ez2fix-cream rounded-2xl p-1 m-1">
              <div class="bg-ez2fix-cream rounded-xl p-6">
                <h3 class="text-2xl font-bold text-ez2fix-dark mb-2">Get Free Quote</h3>
                <p class="text-ez2fix-brown mb-6">Commercial service consultation</p>
                <BookingFormClean variant="hero" />
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- Commercial Scenarios - Problem/Solution/Result -->
    <section class="py-16 md:py-24 bg-white">
      <div class="container mx-auto px-4">
        <div class="max-w-7xl mx-auto">
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl font-bold text-ez2fix-dark mb-4">
              When You Need <span class="text-ez2fix-primary">Commercial Service</span>
            </h2>
            <p class="text-lg md:text-xl text-ez2fix-brown max-w-3xl mx-auto">
              From loading dock emergencies to facility-wide upgrades, professional commercial service keeps operations running smoothly.
            </p>
          </div>

          <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
            {commercialScenarios.map((scenario) => (
              <div class="relative group">
                <div class="absolute inset-0 bg-gradient-to-br from-ez2fix-primary to-ez2fix-gold rounded-2xl transform group-hover:scale-105 transition-transform duration-300"></div>
                <div class="relative bg-white rounded-2xl p-8 m-1">
                  <div class="w-16 h-16 bg-gradient-to-br from-ez2fix-primary to-ez2fix-gold rounded-xl flex items-center justify-center mb-6 transform group-hover:rotate-12 transition-transform duration-300">
                    {scenario.icon === 'dock' && (
                      <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
                      </svg>
                    )}
                    {scenario.icon === 'cycle' && (
                      <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15"/>
                      </svg>
                    )}
                    {scenario.icon === 'energy' && (
                      <svg class="w-8 h-8 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"/>
                      </svg>
                    )}
                  </div>

                  <h3 class="text-2xl font-bold text-ez2fix-dark mb-4">{scenario.scenario}</h3>

                  <div class="space-y-3 mb-6">
                    <div class="flex items-start">
                      <span class="text-red-600 font-bold mr-2 mt-1">Problem:</span>
                      <span class="text-ez2fix-brown">{scenario.problem}</span>
                    </div>
                    <div class="flex items-start">
                      <span class="text-ez2fix-primary font-bold mr-2 mt-1">Solution:</span>
                      <span class="text-ez2fix-brown">{scenario.solution}</span>
                    </div>
                    <div class="flex items-start">
                      <span class="text-green-600 font-bold mr-2 mt-1">Result:</span>
                      <span class="text-ez2fix-dark font-semibold">{scenario.benefit}</span>
                    </div>
                  </div>
                </div>
              </div>
            ))}
          </div>
        </div>
      </div>
    </section>

    <!-- Why Commercial Service -->
    <section class="py-16 md:py-24 bg-gradient-to-br from-ez2fix-brown via-ez2fix-dark to-ez2fix-brown">
      <div class="container mx-auto px-4">
        <div class="max-w-7xl mx-auto">
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl font-bold text-white mb-4">
              Why <span class="text-ez2fix-primary">Commercial Service</span> Is Different
            </h2>
            <p class="text-lg md:text-xl text-ez2fix-cream max-w-3xl mx-auto">
              Commercial facilities need specialized expertise, industrial-grade components, and service that prioritizes business continuity.
            </p>
          </div>

          <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
            {whyCommercial.map((reason) => (
              <div class="bg-white/10 backdrop-blur-sm rounded-2xl p-6 border border-white/20 hover:bg-white/20 transition-all duration-300">
                <div class="text-4xl font-bold text-ez2fix-primary mb-3">{reason.stat}</div>
                <h3 class="text-xl font-bold text-white mb-3">{reason.title}</h3>
                <p class="text-ez2fix-cream leading-relaxed">{reason.description}</p>
              </div>
            ))}
          </div>
        </div>
      </div>
    </section>

    <!-- Commercial Services with Alternating Layout -->
    <section class="py-16 md:py-24 bg-gradient-to-br from-ez2fix-cream to-white">
      <div class="container mx-auto px-4">
        <div class="max-w-7xl mx-auto">
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl font-bold text-ez2fix-dark mb-4">
              <span class="text-ez2fix-primary">Commercial Service</span> Solutions
            </h2>
            <p class="text-lg md:text-xl text-ez2fix-brown max-w-3xl mx-auto">
              Comprehensive commercial door solutions from industrial installation to loading dock equipment and preventive maintenance.
            </p>
          </div>

          <div class="space-y-16">
            {commercialServices.map((service, index) => (
              <div class={`grid grid-cols-1 lg:grid-cols-2 gap-8 items-center ${index % 2 === 1 ? 'lg:grid-flow-dense' : ''}`}>
                <div class={index % 2 === 1 ? 'lg:col-start-2' : ''}>
                  <div class="relative">
                    <div class="absolute -inset-4 bg-gradient-to-r from-ez2fix-primary to-ez2fix-gold rounded-2xl opacity-20 blur-xl"></div>
                    <div class="relative bg-white rounded-2xl p-8 shadow-2xl border-2 border-ez2fix-primary/20">
                      <div class="inline-block px-4 py-1 bg-ez2fix-primary/10 text-ez2fix-primary font-bold rounded-full text-sm mb-4">
                        COMMERCIAL WARRANTY
                      </div>
                      <h3 class="text-3xl font-bold text-ez2fix-dark mb-3">{service.title}</h3>
                      <p class="text-2xl text-ez2fix-primary font-bold mb-4">{service.tagline}</p>
                      <p class="text-lg text-ez2fix-brown mb-6 leading-relaxed">{service.description}</p>

                      <div class="space-y-3">
                        {service.benefits.map((benefit) => (
                          <div class="flex items-center">
                            <svg class="w-6 h-6 text-ez2fix-primary mr-3 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                            </svg>
                            <span class="text-ez2fix-dark font-medium">{benefit}</span>
                          </div>
                        ))}
                      </div>
                    </div>
                  </div>
                </div>

                <div class={index % 2 === 1 ? 'lg:col-start-1 lg:row-start-1' : ''}>
                  <div class="relative h-96 rounded-2xl overflow-hidden shadow-2xl">
                    <div class="absolute inset-0 bg-gradient-to-br from-ez2fix-dark to-ez2fix-brown opacity-90"></div>
                    <div class="absolute inset-0 flex items-center justify-center">
                      {service.image === 'industrial' && (
                        <svg class="w-32 h-32 text-ez2fix-primary opacity-50" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 21V5a2 2 0 00-2-2H7a2 2 0 00-2 2v16m14 0h2m-2 0h-5m-9 0H3m2 0h5M9 7h1m-1 4h1m4-4h1m-1 4h1m-5 10v-5a1 1 0 011-1h2a1 1 0 011 1v5m-4 0h4"/>
                        </svg>
                      )}
                      {service.image === 'dock' && (
                        <svg class="w-32 h-32 text-ez2fix-primary opacity-50" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
                        </svg>
                      )}
                      {service.image === 'specialty' && (
                        <svg class="w-32 h-32 text-ez2fix-primary opacity-50" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"/>
                        </svg>
                      )}
                    </div>
                  </div>
                </div>
              </div>
            ))}
          </div>
        </div>
      </div>
    </section>

    <!-- Unique Commercial Reviews -->
    <section class="py-16 md:py-24 bg-white">
      <div class="container mx-auto px-4">
        <div class="max-w-7xl mx-auto">
          <div class="text-center mb-12">
            <h2 class="text-3xl md:text-4xl font-bold text-ez2fix-dark mb-4">
              Commercial Service <span class="text-ez2fix-primary">Success Stories</span>
            </h2>
            <p class="text-lg md:text-xl text-ez2fix-brown max-w-3xl mx-auto">
              Real experiences from New Jersey businesses who trust us for their commercial door service needs.
            </p>
          </div>

          <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8 mb-8">
            {commercialReviews.map((review) => (
              <div class="bg-white rounded-2xl p-6 shadow-xl hover:shadow-2xl transition-all duration-300 hover:-translate-y-2 border-2 border-ez2fix-primary/10">
                <div class="flex text-ez2fix-gold mb-4">
                  {[...Array(5)].map(() => (
                    <svg class="w-5 h-5 fill-current" viewBox="0 0 24 24">
                      <path d="M12 .587l3.668 7.431 8.332 1.151-6.064 5.828 1.48 8.279-7.416-3.967-7.417 3.967 1.481-8.279-6.064-5.828 8.332-1.151z"/>
                    </svg>
                  ))}
                </div>
                <p class="text-ez2fix-dark leading-relaxed mb-4 text-lg italic">"{review.text}"</p>
                <div class="border-t-2 border-ez2fix-cream pt-4">
                  <p class="font-bold text-ez2fix-dark text-lg">{review.author}</p>
                  <p class="text-sm text-ez2fix-brown">{review.location}</p>
                  <div class="mt-2 inline-block px-3 py-1 bg-ez2fix-primary/10 text-ez2fix-primary text-xs font-bold rounded-full">
                    {review.service}
                  </div>
                </div>
              </div>
            ))}
          </div>

          <div class="text-center">
            <a
              href="/reviews"
              class="inline-flex items-center px-8 py-4 bg-ez2fix-primary text-ez2fix-dark hover:bg-ez2fix-gold font-bold text-lg rounded-xl shadow-xl hover:shadow-2xl transition-all duration-300 hover:scale-105"
            >
