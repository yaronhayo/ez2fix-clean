        <div class="max-w-7xl mx-auto">
          
          <div class="section-header-responsive">
            <h2 class="section-title-responsive text-ez2fix-dark">
              Our <span class="text-ez2fix-primary">Garage Door Services</span>
            </h2>
            <p class="section-subtitle-responsive text-ez2fix-brown">
              Licensed contractor (#13VH13553300) serving 12,000+ families across New Jersey with comprehensive expertise and quality workmanship.
            </p>
          </div>

          <!-- Enhanced Service Cards Grid -->
          <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
            
            {services.map((service, index) => (
              <div class={`group relative overflow-hidden rounded-2xl shadow-lg hover:shadow-2xl transition-all duration-500 hover:-translate-y-2 ${
                service.popular ? 'border-3 border-ez2fix-primary bg-gradient-to-br from-white via-ez2fix-cream/50 to-white' : 
                'border-2 border-ez2fix-brown/20 bg-gradient-to-br from-ez2fix-cream to-white hover:border-ez2fix-primary/40'
              }`}>
                
                {/* Popular Badge */}
                {service.popular && (
                  <div class="absolute -top-2 -right-2 z-10">
                    <div class="bg-ez2fix-primary text-white text-xs font-bold px-4 py-2 rounded-full shadow-lg animate-pulse">
                      MOST POPULAR
                    </div>
                  </div>
                )}

                {/* Service Badge */}
                <div class="absolute top-4 left-4 z-10">
                  <span class={`inline-block px-3 py-1 rounded-full text-xs font-bold shadow-sm ${
                    service.badge === '24/7 AVAILABLE' ? 'bg-red-100 text-red-800' :
                    service.badge === 'MOST COMMON' ? 'bg-orange-100 text-orange-800' :
                    service.badge === 'SMART HOME' ? 'bg-blue-100 text-blue-800' :
                    service.badge === 'CURB APPEAL' ? 'bg-green-100 text-green-800' :
                    service.badge === 'BUSINESS' ? 'bg-purple-100 text-purple-800' :
                    'bg-gray-100 text-gray-800'
                  }`}>
                    {service.badge}
                  </span>
                </div>

                {/* Hero Image */}
                <div class="relative h-48 overflow-hidden">
                  <img 
                    src={service.image} 
                    alt={service.title}
                    class="w-full h-full object-cover transition-transform duration-500 group-hover:scale-110"
                    loading="lazy"
                  />
                  <div class="absolute inset-0 bg-gradient-to-t from-black/50 to-transparent"></div>
                  
                  {/* Overlay CTA */}
                  <div class="absolute bottom-4 left-4 right-4 opacity-0 group-hover:opacity-100 transition-opacity duration-300">
                    <div class="bg-white/90 backdrop-blur-sm rounded-lg p-3">
                      <div class="flex items-center justify-between">
                        <span class="text-sm font-medium text-ez2fix-dark">View Service Details</span>
                        <svg class="w-4 h-4 text-ez2fix-primary" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5l7 7-7 7"/>
                        </svg>
                      </div>
                    </div>
                  </div>
                </div>

                {/* Card Content */}
                <div class="p-6">
                  <div class="mb-4">
                    <h3 class="text-heading-3 text-ez2fix-dark mb-2 group-hover:text-ez2fix-primary transition-colors">
                      {service.title}
                    </h3>
                    <p class="text-body-small text-ez2fix-brown leading-relaxed line-clamp-3">
                      {service.description}
                    </p>
                  </div>

                  {/* Features List */}
                  <div class="space-y-2 mb-6">
                    {service.features.map((feature) => (
                      <div class="flex items-center space-x-2">
                        <svg class="w-4 h-4 text-ez2fix-primary flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"/>
                        </svg>
                        <span class="text-responsive-sm text-ez2fix-dark font-medium">{feature}</span>
                      </div>
                    ))}
                  </div>

                  {/* Warranty & CTA */}
                  <div class="border-t border-ez2fix-brown/20 pt-4">
                    <div class="flex items-center justify-between mb-4">
                      <div class="flex items-center space-x-1">
                        <svg class="w-4 h-4 text-ez2fix-gold" fill="currentColor" viewBox="0 0 20 20">
                          <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
